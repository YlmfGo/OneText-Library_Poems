[
  {
		"text": "落絮无声春堕泪，行云有影月含羞。",
		"from": "《浣溪沙·门隔花深梦旧游》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "慈母倚门情，游子行路苦。",
		"from": "《墨萱图·其一》",
		"by": "王冕",
		"time": ["2019.8.20"]
	},
	{
		"text": "窗前竹叶，凛凛狂风折。",
		"from": "《满路花·冬》",
		"by": "张淑芳",
		"time": ["2019.8.20"]
	},
	{
		"text": "遥知湖上一樽酒，能忆天涯万里人。",
		"from": "《春日西湖寄谢法曹歌》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "井放辘轳闲浸酒，笼开鹦鹉报煎茶。",
		"from": "《夏日题老将林亭》",
		"by": "张蠙",
		"time": ["2019.8.20"]
	},
	{
		"text": "蝉噪林逾静，鸟鸣山更幽。",
		"from": "《入若耶溪》",
		"by": "王籍",
		"time": ["2019.8.20"]
	},
	{
		"text": "商人重利轻别离，前月浮梁买茶去。",
		"from": "《琵琶行 / 琵琶引》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "登高望蓬流，想象金银台。",
		"from": "《游泰山六首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "烟尘犯雪岭，鼓角动江城。",
		"from": "《岁暮》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "落花夜雨辞寒食。尘香明日城南陌。",
		"from": "《菩萨蛮·落花夜雨辞寒食》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "石壕村里夫妻别，泪比长生殿上多。",
		"from": "《马嵬》",
		"by": "袁枚",
		"time": ["2019.8.20"]
	},
	{
		"text": "空床卧听南窗雨，谁复挑灯夜补衣。",
		"from": "《半死桐·重过阊门万事非》",
		"by": "贺铸",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河之水天上来，奔流到海不复回。",
		"from": "《将进酒》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "雨后寒轻，风前香软，春在梨花。",
		"from": "《柳梢青·吴中》",
		"by": "仲殊",
		"time": ["2019.8.20"]
	},
	{
		"text": "东风不管琵琶怨。落花吹遍。",
		"from": "《后庭花·一春不识西湖面》",
		"by": "许棐",
		"time": ["2019.8.20"]
	},
	{
		"text": "朝辞白帝彩云间，千里江陵一日还。",
		"from": "《早发白帝城 / 白帝下江陵》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "及时当勉励，岁月不待人。",
		"from": "《杂诗·人生无根蒂》",
		"by": "陶渊明",
		"time": ["2019.8.20"]
	},
	{
		"text": "无言暗将红泪弹。阑珊，香销轻梦还。",
		"from": "《河传·春浅》",
		"by": "纳兰性德",
		"time": ["2019.8.20"]
	},
	{
		"text": "渭城朝雨浥轻尘，客舍青青柳色新。",
		"from": "《渭城曲 / 送元二使安西》",
		"by": "王维",
		"time": ["2019.8.20"]
	},
	{
		"text": "岁暮百草零，疾风高冈裂。",
		"from": "《自京赴奉先县咏怀五百字 / 自京赴奉先咏怀五百字》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "候馆梅残，溪桥柳细。",
		"from": "《踏莎行·候馆梅残》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "日高烟敛，更看今日晴未。",
		"from": "《念奴娇·春情》",
		"by": "李清照",
		"time": ["2019.8.20"]
	},
	{
		"text": "冠盖满京华，斯人独憔悴。",
		"from": "《梦李白二首·其二》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "愁颜与衰鬓，明日又逢春。",
		"from": "《除夜宿石头驿》",
		"by": "戴叔伦",
		"time": ["2019.8.20"]
	},
	{
		"text": "无边落木萧萧下，不尽长江滚滚来。",
		"from": "《登高》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "休对故人思故国，且将新火试新茶。诗酒趁年华。",
		"from": "《望江南·超然台作》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "留恋海棠颜色、过清明。",
		"from": "《南歌子·柳户朝云湿》",
		"by": "张辑",
		"time": ["2019.8.20"]
	},
	{
		"text": "高节志凌云，不敢当滕六。",
		"from": "《薛宝钗·雪竹》",
		"by": "环山樵",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉瀑布遥相望，回崖沓嶂凌苍苍。",
		"from": "《庐山谣寄卢侍御虚舟》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉初上日，瀑水喷成虹。",
		"from": "《彭蠡湖中望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "竹坞无尘水槛清，相思迢递隔重城。",
		"from": "《宿骆氏亭寄怀崔雍崔衮》",
		"by": "李商隐",
		"time": ["2019.8.20"]
	},
	{
		"text": "年年今夜，月华如练，长是人千里。",
		"from": "《御街行·秋日怀旧》",
		"by": "范仲淹",
		"time": ["2019.8.20"]
	},
	{
		"text": "羌管悠悠霜满地，人不寐，将军白发征夫泪。",
		"from": "《渔家傲·秋思》",
		"by": "范仲淹",
		"time": ["2019.8.20"]
	},
	{
		"text": "骑牛远远过前村，短笛横吹隔陇闻。",
		"from": "《牧童诗》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "梅须逊雪三分白，雪却输梅一段香。",
		"from": "《雪梅·其一》",
		"by": "卢梅坡",
		"time": ["2019.8.20"]
	},
	{
		"text": "一去无消息，那能惜马蹄。",
		"from": "《昔昔盐》",
		"by": "薛道衡",
		"time": ["2019.8.20"]
	},
	{
		"text": "劝君频入醉乡来，此是无愁无恨处。",
		"from": "《玉楼春·雕鞍好为莺花住》",
		"by": "晏几道",
		"time": ["2019.8.20"]
	},
	{
		"text": "山上层层桃李花，云间烟火是人家。",
		"from": "《竹枝词九首·其九》",
		"by": "刘禹锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "东篱把酒黄昏后，有暗香盈袖。",
		"from": "《醉花阴·薄雾浓云愁永昼》",
		"by": "李清照",
		"time": ["2019.8.20"]
	},
	{
		"text": "短篱残菊一枝黄。正是乱山深处、过重阳。",
		"from": "《南歌子·驿路侵斜月》",
		"by": "吕本中",
		"time": ["2019.8.20"]
	},
	{
		"text": "洛阳城里春光好，洛阳才子他乡老。",
		"from": "《菩萨蛮》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "群芳过后西湖好，狼籍残红。",
		"from": "《采桑子·群芳过后西湖好》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "四海皆兄弟，谁为行路人。",
		"from": "《旧题苏武诗 / 别诗四首·其一》",
		"by": "佚名",
		"time": ["2019.8.20"]
	},
	{
		"text": "万物迎春送残腊，一年结局在今宵。",
		"from": "《除夜》",
		"by": "戴复古",
		"time": ["2019.8.20"]
	},
	{
		"text": "墉集欺猫鼠，林藏逐雀鹯。",
		"from": "《中秋咏怀借杜子美秋日述怀一百韵和寄柳州假鸣桑先生》",
		"by": "徐威",
		"time": ["2019.8.20"]
	},
	{
		"text": "好梦欲成还又觉，绿窗但觉莺啼晓。",
		"from": "《蝶恋花·一别家山音信杳》",
		"by": "施耐庵",
		"time": ["2019.8.20"]
	},
	{
		"text": "有孙母未去，出入无完裙。",
		"from": "《石壕吏》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "昨夜斗回北，今朝岁起东。",
		"from": "《田家元日》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "唤起封姨清晚景，更将荔子荐新圆。",
		"from": "《浣溪沙·中秋坐上十八客》",
		"by": "张孝祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "缀玉联珠六十年，谁教冥路作诗仙。",
		"from": "《吊白居易》",
		"by": "李忱",
		"time": ["2019.8.20"]
	},
	{
		"text": "羌笛何须怨杨柳，春风不度玉门关。",
		"from": "《凉州词二首·其一》",
		"by": "王之涣",
		"time": ["2019.8.20"]
	},
	{
		"text": "君歌杨叛儿，妾劝新丰酒。",
		"from": "《杨叛儿》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "柳絮风轻，梨花雨细。",
		"from": "《踏莎行·柳絮风轻》",
		"by": "谢逸",
		"time": ["2019.8.20"]
	},
	{
		"text": "被酒莫惊春睡重，赌书消得泼茶香，当时只道是寻常。",
		"from": "《浣溪沙·谁念西风独自凉》",
		"by": "纳兰性德",
		"time": ["2019.8.20"]
	},
	{
		"text": "雪虐风饕愈凛然，花中气节最高坚。",
		"from": "《落梅》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "一棹碧涛春水路。过尽晓莺啼处。",
		"from": "《清平乐·留人不住》",
		"by": "晏几道",
		"time": ["2019.8.20"]
	},
	{
		"text": "昨夜西风凋碧树。独上高楼，望尽天涯路。",
		"from": "《蝶恋花·槛菊愁烟兰泣露》",
		"by": "晏殊",
		"time": ["2019.8.20"]
	},
	{
		"text": "爱向竹栏骑竹马，懒于金地聚金沙。",
		"from": "《送童子下山》",
		"by": "金地藏",
		"time": ["2019.8.20"]
	},
	{
		"text": "秋菊有佳色，裛露掇其英。",
		"from": "《饮酒·其四》",
		"by": "陶渊明",
		"time": ["2019.8.20"]
	},
	{
		"text": "有熊有罴，有猫有虎，庆既令居，韩姞燕誉。",
		"from": "《韩奕》",
		"by": "佚名",
		"time": ["2019.8.20"]
	},
	{
		"text": "雨色秋来寒，风严清江爽。",
		"from": "《酬裴侍御对雨感时见赠》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "陪都歌舞迎佳节，遥祝延安景物华。",
		"from": "《元旦口占用柳亚子怀人韵》",
		"by": "董必武",
		"time": ["2019.8.20"]
	},
	{
		"text": "彩线轻缠红玉臂，小符斜挂绿云鬟。",
		"from": "《浣溪沙·端午》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "一日不读书，胸臆无佳想。",
		"from": "《读书有所见作》",
		"by": "萧抡谓",
		"time": ["2019.8.20"]
	},
	{
		"text": "阴晴圆缺都休说，且喜人间好时节。",
		"from": "《中秋月·中秋月》",
		"by": "徐有贞",
		"time": ["2019.8.20"]
	},
	{
		"text": "世间珍果更无加，玉雪肌肤罩绛纱。",
		"from": "《咏荔枝》",
		"by": "丘浚",
		"time": ["2019.8.20"]
	},
	{
		"text": "岭南能有几多花，寒食临之扫春迹。",
		"from": "《寒食》",
		"by": "郑刚中",
		"time": ["2019.8.20"]
	},
	{
		"text": "霜殒芦花泪湿衣，白头无复倚柴扉。",
		"from": "《思母》",
		"by": "与恭",
		"time": ["2019.8.20"]
	},
	{
		"text": "我欲穿花寻路，直入白云深处，浩气展虹霓。",
		"from": "《水调歌头·游览》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "庭前时有东风入，杨柳千条尽向西。",
		"from": "《代春怨》",
		"by": "刘方平",
		"time": ["2019.8.20"]
	},
	{
		"text": "杏花时节雨纷纷。山绕孤村。水绕孤村。",
		"from": "《一剪梅·唱到阳关第四声》",
		"by": "刘仙伦",
		"time": ["2019.8.20"]
	},
	{
		"text": "一生傲岸苦不谐，恩疏媒劳志多乖。",
		"from": "《答王十二寒夜独酌有怀》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "琵琶金翠羽，弦上黄莺语。",
		"from": "《菩萨蛮》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "位卑未敢忘忧国，事定犹须待阖棺。",
		"from": "《病起书怀》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "却下水晶帘，玲珑望秋月。",
		"from": "《玉阶怨》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "树阴满地日当午，梦觉流莺时一声。",
		"from": "《夏意》",
		"by": "苏舜钦",
		"time": ["2019.8.20"]
	},
	{
		"text": "挂席几千里，名山都未逢。",
		"from": "《晚泊浔阳望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "灯火万家城四畔，星河一道水中央。",
		"from": "《江楼夕望招客》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "最喜小儿亡赖，溪头卧剥莲蓬。",
		"from": "《清平乐·村居》",
		"by": "辛弃疾",
		"time": ["2019.8.20"]
	},
	{
		"text": "大漠风尘日色昏，红旗半卷出辕门。",
		"from": "《从军行七首》",
		"by": "王昌龄",
		"time": ["2019.8.20"]
	},
	{
		"text": "芙蓉老秋霜，团扇羞网尘。",
		"from": "《中山孺子妾歌》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "唤起封姨清晚景，更将荔子荐新圆。",
		"from": "《浣溪沙·中秋坐上十八客》",
		"by": "张孝祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "悲怀感物来，泣涕应情陨。",
		"from": "《悼亡诗三首》",
		"by": "潘安",
		"time": ["2019.8.20"]
	},
	{
		"text": "寒沙四面平，飞雪千里惊。",
		"from": "《效古诗》",
		"by": "范云",
		"time": ["2019.8.20"]
	},
	{
		"text": "不识庐山真面目，只缘身在此山中。",
		"from": "《题西林壁》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "摇落深知宋玉悲，风流儒雅亦吾师。",
		"from": "《咏怀古迹五首·其二》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河西来决昆仑，咆哮万里触龙门。",
		"from": "《公无渡河》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "伏波惟愿裹尸还，定远何须生入关。",
		"from": "《塞下曲》",
		"by": "李益",
		"time": ["2019.8.20"]
	},
	{
		"text": "千门万户曈曈日，总把新桃换旧符。",
		"from": "《元日》",
		"by": "王安石",
		"time": ["2019.8.20"]
	},
	{
		"text": "不似春风逞红艳，镜前空坠玉人钗。",
		"from": "《华下对菊》",
		"by": "司空图",
		"time": ["2019.8.20"]
	},
	{
		"text": "入春才七日，离家已二年。",
		"from": "《人日思归》",
		"by": "薛道衡",
		"time": ["2019.8.20"]
	},
	{
		"text": "惟有年时芳俦在，一例差池双剪。",
		"from": "《金缕曲·丁未五月归国旋复东渡却寄沪上诸君子》",
		"by": "梁启超",
		"time": ["2019.8.20"]
	},
	{
		"text": "秋风吹到江村，正黄昏，寂寞梧桐夜雨不开门。",
		"from": "《相见欢·秋风吹到江村》",
		"by": "顾彩",
		"time": ["2019.8.20"]
	},
	{
		"text": "喜遇重阳，更佳酿今朝新熟。",
		"from": "《满江红·喜遇重阳》",
		"by": "宋江",
		"time": ["2019.8.20"]
	},
	{
		"text": "斜月照帘帷，忆君和梦稀。",
		"from": "《菩萨蛮·梨花满院飘香雪》",
		"by": "毛熙震",
		"time": ["2019.8.20"]
	},
	{
		"text": "霜殒芦花泪湿衣，白头无复倚柴扉。",
		"from": "《思母》",
		"by": "与恭",
		"time": ["2019.8.20"]
	},
	{
		"text": "楼船夜雪瓜洲渡，铁马秋风大散关。",
		"from": "《书愤五首·其一》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "初惊河汉落，半洒云天里。",
		"from": "《望庐山瀑布水二首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "江雨霏霏江草齐，六朝如梦鸟空啼。",
		"from": "《台城》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "人言落日是天涯，望极天涯不见家。",
		"from": "《乡思》",
		"by": "李觏",
		"time": ["2019.8.20"]
	},
	{
		"text": "曾经沧海难为水，除却巫山不是云。",
		"from": "《离思五首·其四》",
		"by": "元稹",
		"time": ["2019.8.20"]
	},
	{
		"text": "古宫闲地少，水港小桥多。",
		"from": "《送人游吴》",
		"by": "杜荀鹤",
		"time": ["2019.8.20"]
	},
	{
		"text": "骑牛远远过前村，短笛横吹隔陇闻。",
		"from": "《牧童诗》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "萧萧落木不胜秋，莫回首、斜阳下。",
		"from": "《一络索·过尽遥山如画》",
		"by": "纳兰性德",
		"time": ["2019.8.20"]
	},
	{
		"text": "惟有今宵，皓彩皆同普。",
		"from": "《醉落魄·丙寅中秋》",
		"by": "郭应祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "戍鼓断人行，边秋一雁声。",
		"from": "《月夜忆舍弟》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "商人重利轻别离，前月浮梁买茶去。",
		"from": "《琵琶行 / 琵琶引》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "渐老多忧百事忙，天寒日短更心伤。",
		"from": "《秋晚悲怀》",
		"by": "李觏",
		"time": ["2019.8.20"]
	},
	{
		"text": "哀哀父母，生我劳瘁。",
		"from": "《蓼莪》",
		"by": "佚名",
		"time": ["2019.8.20"]
	},
	{
		"text": "苔深不能扫，落叶秋风早。",
		"from": "《长干行二首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "灯前目力虽非昔，犹课蝇头二万言。",
		"from": "《读书》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "戍鼓断人行，边秋一雁声。",
		"from": "《月夜忆舍弟》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "可有梅花寄一枝？雪来翠羽飞。",
		"from": "《长相思·以书寄西泠诸友即题其后》",
		"by": "吴锡麒",
		"time": ["2019.8.20"]
	},
	{
		"text": "断崖苍藓对立久，冻雨为洗前朝悲。",
		"from": "《书摩崖碑后》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "伤心千里江南，怨曲重招，断魂在否？",
		"from": "《莺啼序·春晚感怀》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄沙百战穿金甲，不破楼兰终不还。",
		"from": "《从军行七首·其四》",
		"by": "王昌龄",
		"time": ["2019.8.20"]
	},
	{
		"text": "身既死兮神以灵，魂魄毅兮为鬼雄。",
		"from": "《国殇》",
		"by": "屈原",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河捧土尚可塞，北风雨雪恨难裁。",
		"from": "《北风行》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "泰山不要欺毫末，颜子无心羡老彭。",
		"from": "《放言五首·其五》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉初上日，瀑水喷成虹。",
		"from": "《彭蠡湖中望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河水，水阔无边深无底，其来不知几千里。",
		"from": "《黄河行》",
		"by": "贡泰父",
		"time": ["2019.8.20"]
	},
	{
		"text": "霾两轮兮絷四马，援玉枹兮击鸣鼓。",
		"from": "《国殇》",
		"by": "屈原",
		"time": ["2019.8.20"]
	},
	{
		"text": "乱山残雪夜，孤烛异乡人。",
		"from": "《除夜 / 巴山道中除夜书怀 / 除夜有怀》",
		"by": "崔涂",
		"time": ["2019.8.20"]
	},
	{
		"text": "来时父母知隔生，重著衣裳如送死。",
		"from": "《渡辽水》",
		"by": "王建",
		"time": ["2019.8.20"]
	},
	{
		"text": "云中谁寄锦书来，雁字回时，月满西楼。",
		"from": "《一剪梅·红藕香残玉簟秋》",
		"by": "李清照",
		"time": ["2019.8.20"]
	},
	{
		"text": "楼船夜雪瓜洲渡，铁马秋风大散关。",
		"from": "《书愤五首·其一》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉初上日，瀑水喷成虹。",
		"from": "《彭蠡湖中望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "怀家寒食夜，中酒落花天。",
		"from": "《临江仙·暮春》",
		"by": "赵长卿",
		"time": ["2019.8.20"]
	},
	{
		"text": "娶妻无媒毋须恨，书中有女颜如玉。",
		"from": "《劝学诗》",
		"by": "赵恒",
		"time": ["2019.8.20"]
	},
	{
		"text": "河海不择细流，故能就其深；",
		"from": "《谏逐客书》",
		"by": "李斯",
		"time": ["2019.8.20"]
	},
	{
		"text": "满月飞明镜，归心折大刀。",
		"from": "《八月十五夜月二首》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "此生此夜不长好，明月明年何处看。",
		"from": "《阳关曲 / 中秋月》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "一轮秋影转金波。飞镜又重磨。",
		"from": "《太常引·建康中秋夜为吕叔潜赋》",
		"by": "辛弃疾",
		"time": ["2019.8.20"]
	},
	{
		"text": "离别一何久，七度过中秋。",
		"from": "《水调歌头·徐州中秋》",
		"by": "苏辙",
		"time": ["2019.8.20"]
	},
	{
		"text": "初惊河汉落，半洒云天里。",
		"from": "《望庐山瀑布水二首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉初上日，瀑水喷成虹。",
		"from": "《彭蠡湖中望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "归装渐理君知否？笑指庐山古涧藤。",
		"from": "《自咏示客》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "涛澜汹涌，风云开阖。",
		"from": "《黄州快哉亭记》",
		"by": "苏辙",
		"time": ["2019.8.20"]
	},
	{
		"text": "有情风、万里卷潮来，无情送潮归。",
		"from": "《八声甘州·寄参寥子》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河万里触山动，盘涡毂转秦地雷。",
		"from": "《西岳云台歌送丹丘子》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "长江巨浪征人泪，一夜西风共白头。",
		"from": "《江上阻风》",
		"by": "宋琬",
		"time": ["2019.8.20"]
	},
	{
		"text": "九月九日望乡台，他席他乡送客杯。",
		"from": "《蜀中九日 / 九日登高》",
		"by": "王勃",
		"time": ["2019.8.20"]
	},
	{
		"text": "激气已能驱粉黛，举杯便可吞吴越。",
		"from": "《满江红·中秋夜潮》",
		"by": "史达祖",
		"time": ["2019.8.20"]
	},
	{
		"text": "所志在功名，离别何足叹。",
		"from": "《别离》",
		"by": "陆龟蒙",
		"time": ["2019.8.20"]
	},
	{
		"text": "昆山玉碎凤凰叫，芙蓉泣露香兰笑。",
		"from": "《李凭箜篌引》",
		"by": "李贺",
		"time": ["2019.8.20"]
	},
	{
		"text": "岸雨过城头，黄鹂上戍楼。",
		"from": "《武威春暮闻宇文判官西使还已到晋昌》",
		"by": "岑参",
		"time": ["2019.8.20"]
	},
	{
		"text": "日啖荔枝三百颗，不辞长作岭南人。",
		"from": "《惠州一绝》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "自闻颖师弹，起坐在一旁。",
		"from": "《听颖师弹琴》",
		"by": "韩愈",
		"time": ["2019.8.20"]
	},
	{
		"text": "烽火照西京，心中自不平。",
		"from": "《从军行》",
		"by": "杨炯",
		"time": ["2019.8.20"]
	},
	{
		"text": "兰有秀兮菊有芳，怀佳人兮不能忘。",
		"from": "《秋风辞》",
		"by": "刘彻",
		"time": ["2019.8.20"]
	},
	{
		"text": "惟将终夜常开眼，报答平生未展眉。",
		"from": "《遣悲怀三首·其三》",
		"by": "元稹",
		"time": ["2019.8.20"]
	},
	{
		"text": "路漫漫其修远兮，吾将上下而求索。",
		"from": "《离骚》",
		"by": "屈原",
		"time": ["2019.8.20"]
	},
	{
		"text": "松花酿酒，春水煎茶。",
		"from": "《人月圆·山中书事》",
		"by": "张可久",
		"time": ["2019.8.20"]
	},
	{
		"text": "碧玉妆成一树高，万条垂下绿丝绦。",
		"from": "《咏柳 / 柳枝词》",
		"by": "贺知章",
		"time": ["2019.8.20"]
	},
	{
		"text": "惟有今宵，皓彩皆同普。",
		"from": "《醉落魄·丙寅中秋》",
		"by": "郭应祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "儿童强不睡，相守夜欢哗。",
		"from": "《守岁》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "唤起封姨清晚景，更将荔子荐新圆。",
		"from": "《浣溪沙·中秋坐上十八客》",
		"by": "张孝祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "星垂平野阔，月涌大江流。",
		"from": "《旅夜书怀》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "读书不觉已春深，一寸光阴一寸金。",
		"from": "《白鹿洞二首·其一》",
		"by": "王贞白",
		"time": ["2019.8.20"]
	},
	{
		"text": "何日请缨提锐旅，一鞭直渡清河洛。",
		"from": "《满江红·登黄鹤楼有感》",
		"by": "岳飞",
		"time": ["2019.8.20"]
	},
	{
		"text": "偶应非熊兆，尊为帝者师。",
		"from": "《题太公钓渭图》",
		"by": "刘基",
		"time": ["2019.8.20"]
	},
	{
		"text": "恨身翻不作车尘，万里得随君。",
		"from": "《巫山一段云·春去秋来也》",
		"by": "欧阳炯",
		"time": ["2019.8.20"]
	},
	{
		"text": "叹息老来交旧尽，睡来谁共午瓯茶。",
		"from": "《幽居初夏》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "马上逢寒食，愁中属暮春。",
		"from": "《途中寒食题黄梅临江驿寄崔融》",
		"by": "宋之问",
		"time": ["2019.8.20"]
	},
	{
		"text": "穿天透地不辞劳，到底方知出处高。",
		"from": "《咏瀑布》",
		"by": "冯云山",
		"time": ["2019.8.20"]
	},
	{
		"text": "喜得故人同待诏，拟沽春酒醉京华。",
		"from": "《清明呈馆中诸公》",
		"by": "高启",
		"time": ["2019.8.20"]
	},
	{
		"text": "卧看牵牛织女星，月转过梧桐树影。",
		"from": "《沉醉东风·七夕》",
		"by": "卢挚",
		"time": ["2019.8.20"]
	},
	{
		"text": "半壁横江矗起，一舟载雨孤行。",
		"from": "《锦堂春·半壁横江矗起》",
		"by": "归庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "重重叠叠上瑶台，几度呼童扫不开。",
		"from": "《花影》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "春城无处不飞花，寒食东风御柳斜。",
		"from": "《寒食 / 寒食日即事》",
		"by": "韩翃",
		"time": ["2019.8.20"]
	},
	{
		"text": "五月虽热麦风清，檐头索索缲车鸣。",
		"from": "《田家行》",
		"by": "王建",
		"time": ["2019.8.20"]
	},
	{
		"text": "万顷风涛不记苏。雪晴江上麦千车。",
		"from": "《浣溪沙·万顷风涛不记苏》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "泛菊杯深，吹梅角远，同在京城。",
		"from": "《柳梢青·送卢梅坡》",
		"by": "刘过",
		"time": ["2019.8.20"]
	},
	{
		"text": "空床卧听南窗雨，谁复挑灯夜补衣。",
		"from": "《半死桐·重过阊门万事非》",
		"by": "贺铸",
		"time": ["2019.8.20"]
	},
	{
		"text": "故人一去无期约。尺书忽寄西飞鹤。",
		"from": "《忆秦娥·季温老友归樵阳人来闲书因以为寄》",
		"by": "孙道绚",
		"time": ["2019.8.20"]
	},
	{
		"text": "微雨众卉新，一雷惊蛰始。",
		"from": "《观田家》",
		"by": "韦应物",
		"time": ["2019.8.20"]
	},
	{
		"text": "随风潜入夜，润物细无声。",
		"from": "《春夜喜雨》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "田田初出水，菡萏念娇蕊。",
		"from": "《菩萨蛮·题画》",
		"by": "龚翔麟",
		"time": ["2019.8.20"]
	},
	{
		"text": "舒卷江山图画，应答龙鱼悲啸，不暇顾诗愁。",
		"from": "《水调歌头·登多景楼》",
		"by": "杨炎正",
		"time": ["2019.8.20"]
	},
	{
		"text": "梦回人远许多愁，只在梨花风雨处。",
		"from": "《玉楼春·风前欲劝春光住》",
		"by": "辛弃疾",
		"time": ["2019.8.20"]
	},
	{
		"text": "玉座犹寂寞，况乃妾身轻。",
		"from": "《同谢谘议咏铜雀台》",
		"by": "谢朓",
		"time": ["2019.8.20"]
	},
	{
		"text": "恨身翻不作车尘，万里得随君。",
		"from": "《巫山一段云·春去秋来也》",
		"by": "欧阳炯",
		"time": ["2019.8.20"]
	},
	{
		"text": "织妇何太忙，蚕经三卧行欲老。",
		"from": "《织妇词》",
		"by": "元稹",
		"time": ["2019.8.20"]
	},
	{
		"text": "商女不知亡国恨，隔江犹唱后庭花。",
		"from": "《泊秦淮》",
		"by": "杜牧",
		"time": ["2019.8.20"]
	},
	{
		"text": "去年相送，馀杭门外，飞雪似杨花。",
		"from": "《少年游·润州作》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "寒食时看郭外春，野人无处不伤神。",
		"from": "《寒食诗》",
		"by": "云表",
		"time": ["2019.8.20"]
	},
	{
		"text": "露叶翻风惊鹊坠。暗落青林红子。",
		"from": "《清平乐·秋光烛地》",
		"by": "陈师道",
		"time": ["2019.8.20"]
	},
	{
		"text": "梨花风起正清明，游子寻春半出城。",
		"from": "《苏堤清明即事》",
		"by": "吴惟信",
		"time": ["2019.8.20"]
	},
	{
		"text": "岁暮阴阳催短景，天涯霜雪霁寒宵。",
		"from": "《阁夜》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "庐山秀出南斗傍，屏风九叠云锦张。",
		"from": "《庐山谣寄卢侍御虚舟》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "扁舟去作江南客，旅雁孤云。",
		"from": "《采桑子·彭浪矶》",
		"by": "朱敦儒",
		"time": ["2019.8.20"]
	},
	{
		"text": "儿女纷纷夸结束，新样钗符艾虎。",
		"from": "《贺新郎·端午》",
		"by": "刘克庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "罩向金笼好羽仪，分明喉舌似君稀。",
		"from": "《咏鹦鹉》",
		"by": "僧定渚",
		"time": ["2019.8.20"]
	},
	{
		"text": "乱云低薄暮，急雪舞回风。",
		"from": "《对雪》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "应知早飘落，故逐上春来。",
		"from": "《咏早梅 / 扬州法曹梅花盛开》",
		"by": "何逊",
		"time": ["2019.8.20"]
	},
	{
		"text": "远书归梦两悠悠，只有空床敌素秋。",
		"from": "《端居》",
		"by": "李商隐",
		"time": ["2019.8.20"]
	},
	{
		"text": "西城杨柳弄春柔。动离忧。泪难收。",
		"from": "《江城子·西城杨柳弄春柔》",
		"by": "秦观",
		"time": ["2019.8.20"]
	},
	{
		"text": "烟水初销见万家，东风吹柳万条斜。",
		"from": "《襄阳寒食寄宇文籍》",
		"by": "窦巩",
		"time": ["2019.8.20"]
	},
	{
		"text": "可有梅花寄一枝？雪来翠羽飞。",
		"from": "《长相思·以书寄西泠诸友即题其后》",
		"by": "吴锡麒",
		"time": ["2019.8.20"]
	},
	{
		"text": "白头空负雪边春，着意问春春不语。",
		"from": "《玉楼春·东山探梅》",
		"by": "刘镇",
		"time": ["2019.8.20"]
	},
	{
		"text": "唤起封姨清晚景，更将荔子荐新圆。",
		"from": "《浣溪沙·中秋坐上十八客》",
		"by": "张孝祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "两水夹明镜，双桥落彩虹。",
		"from": "《秋登宣城谢脁北楼》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "唤起封姨清晚景，更将荔子荐新圆。",
		"from": "《浣溪沙·中秋坐上十八客》",
		"by": "张孝祥",
		"time": ["2019.8.20"]
	},
	{
		"text": "斑竹枝，斑竹枝，泪痕点点寄相思。",
		"from": "《潇湘神·斑竹枝》",
		"by": "刘禹锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "剧辛乐毅感恩分，输肝剖胆效英才。",
		"from": "《行路难三首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "断虹霁雨，净秋空，山染修眉新绿。",
		"from": "《念奴娇·断虹霁雨》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "看蓬门秋草，年年破巷，疏窗细雨，夜夜孤灯。",
		"from": "《沁园春·恨》",
		"by": "郑板桥",
		"time": ["2019.8.20"]
	},
	{
		"text": "功名万里外，心事一杯中。",
		"from": "《送李侍御赴安西》",
		"by": "高适",
		"time": ["2019.8.20"]
	},
	{
		"text": "九日龙山饮，黄花笑逐臣。",
		"from": "《九日龙山饮》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "马蹀阏氏血，旗袅可汗头。",
		"from": "《送紫岩张先生北伐》",
		"by": "岳飞",
		"time": ["2019.8.20"]
	},
	{
		"text": "自闻颖师弹，起坐在一旁。",
		"from": "《听颖师弹琴》",
		"by": "韩愈",
		"time": ["2019.8.20"]
	},
	{
		"text": "渡头杨柳青青。枝枝叶叶离情。",
		"from": "《清平乐·留人不住》",
		"by": "晏几道",
		"time": ["2019.8.20"]
	},
	{
		"text": "十年旧梦无寻处，几度新春不在家。",
		"from": "《思佳客·癸卯除夜》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "欲系青春，少住春还去。",
		"from": "《蝶恋花·送春》",
		"by": "朱淑真",
		"time": ["2019.8.20"]
	},
	{
		"text": "今人不见古时月，今月曾经照古人。",
		"from": "《把酒问月·故人贾淳令予问之》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "赤壁矶头，一番过、一番怀古。",
		"from": "《满江红·赤壁怀古》",
		"by": "戴复古",
		"time": ["2019.8.20"]
	},
	{
		"text": "朗月清风，浓烟暗雨，天教憔悴度芳姿。",
		"from": "《多丽·咏白菊》",
		"by": "李清照",
		"time": ["2019.8.20"]
	},
	{
		"text": "迢迢牵牛星，皎皎河汉女。",
		"from": "《迢迢牵牛星》",
		"by": "佚名",
		"time": ["2019.8.20"]
	},
	{
		"text": "初惊河汉落，半洒云天里。",
		"from": "《望庐山瀑布水二首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "黄河远上白云间，一片孤城万仞山。",
		"from": "《凉州词二首·其一》",
		"by": "王之涣",
		"time": ["2019.8.20"]
	},
	{
		"text": "剑外忽传收蓟北，初闻涕泪满衣裳。",
		"from": "《闻官军收河南河北》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "军中宜剑舞，塞上重笳音。",
		"from": "《幽州夜饮》",
		"by": "张说",
		"time": ["2019.8.20"]
	},
	{
		"text": "红酥手，黄縢酒，满城春色宫墙柳。",
		"from": "《钗头凤·红酥手》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "春衫犹是，小蛮针线，曾湿西湖雨。",
		"from": "《青玉案·送伯固归吴中》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "满园花菊郁金黄，中有孤丛色似霜。",
		"from": "《重阳席上赋白菊》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "云无心以出岫，鸟倦飞而知还。",
		"from": "《归去来兮辞·并序》",
		"by": "陶渊明",
		"time": ["2019.8.20"]
	},
	{
		"text": "君马黄，我马白。",
		"from": "《君马黄》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "诚知此恨人人有，贫贱夫妻百事哀。",
		"from": "《遣悲怀三首·其二》",
		"by": "元稹",
		"time": ["2019.8.20"]
	},
	{
		"text": "枝上柳绵吹又少。天涯何处无芳草。",
		"from": "《蝶恋花·春景》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "中山人予之猫，猫善捕鼠及鸡。",
		"from": "《乞猫》",
		"by": "刘基",
		"time": ["2019.8.20"]
	},
	{
		"text": "半烟半雨溪桥畔，渔翁醉着无人唤。",
		"from": "《菩萨蛮·半烟半雨溪桥畔》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "当年曾胜赏，生香熏袖，活火分茶。",
		"from": "《转调满庭芳·芳草池塘》",
		"by": "李清照",
		"time": ["2019.8.20"]
	},
	{
		"text": "唯有相思似春色，江南江北送君归。",
		"from": "《送沈子归江东 / 送沈子福之江东》",
		"by": "王维",
		"time": ["2019.8.20"]
	},
	{
		"text": "若似月轮终皎洁，不辞冰雪为卿热。",
		"from": "《蝶恋花·辛苦最怜天上月》",
		"by": "纳兰性德",
		"time": ["2019.8.20"]
	},
	{
		"text": "白日依山尽，黄河入海流。",
		"from": "《登鹳雀楼》",
		"by": "王之涣",
		"time": ["2019.8.20"]
	},
	{
		"text": "恐是仙家好别离，故教迢递作佳期。",
		"from": "《辛未七夕》",
		"by": "李商隐",
		"time": ["2019.8.20"]
	},
	{
		"text": "昼夜蔽日月，冬夏共霜雪。",
		"from": "《登庐山绝顶望诸峤》",
		"by": "谢灵运",
		"time": ["2019.8.20"]
	},
	{
		"text": "新竹高于旧竹枝，全凭老干为扶持。",
		"from": "《新竹》",
		"by": "郑燮",
		"time": ["2019.8.20"]
	},
	{
		"text": "父兮生我，母兮鞠我。",
		"from": "《蓼莪》",
		"by": "佚名",
		"time": ["2019.8.20"]
	},
	{
		"text": "独出前门望野田，月明荞麦花如雪。",
		"from": "《村夜》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "中山人予之猫，猫善捕鼠及鸡。",
		"from": "《乞猫》",
		"by": "刘基",
		"time": ["2019.8.20"]
	},
	{
		"text": "凤凰台上凤凰游，凤去台空江自流。",
		"from": "《登金陵凤凰台》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "三百年间同晓梦，钟山何处有龙盘。",
		"from": "《咏史二首·其一》",
		"by": "李商隐",
		"time": ["2019.8.20"]
	},
	{
		"text": "道人庭宇静，苔色连深竹。",
		"from": "《晨诣超师院读禅经》",
		"by": "柳宗元",
		"time": ["2019.8.20"]
	},
	{
		"text": "棘枝风哭酸，桐叶霜颜高。",
		"from": "《秋怀十五首》",
		"by": "孟郊",
		"time": ["2019.8.20"]
	},
	{
		"text": "淅淅西风淡淡烟，几点疏疏雨。",
		"from": "《卜算子·席间再作》",
		"by": "葛立方",
		"time": ["2019.8.20"]
	},
	{
		"text": "云散月明谁点缀？天容海色本澄清。",
		"from": "《六月二十日夜渡海》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "白头空负雪边春，着意问春春不语。",
		"from": "《玉楼春·东山探梅》",
		"by": "刘镇",
		"time": ["2019.8.20"]
	},
	{
		"text": "霸图今已矣，驱马复归来。",
		"from": "《燕昭王》",
		"by": "陈子昂",
		"time": ["2019.8.20"]
	},
	{
		"text": "人老去西风白发，蝶愁来明日黄花。",
		"from": "《折桂令·九日》",
		"by": "张可久",
		"time": ["2019.8.20"]
	},
	{
		"text": "昼夜蔽日月，冬夏共霜雪。",
		"from": "《登庐山绝顶望诸峤》",
		"by": "谢灵运",
		"time": ["2019.8.20"]
	},
	{
		"text": "世间珍果更无加，玉雪肌肤罩绛纱。",
		"from": "《咏荔枝》",
		"by": "丘浚",
		"time": ["2019.8.20"]
	},
	{
		"text": "昔日芙蓉花，今成断根草。",
		"from": "《妾薄命》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "不经一番寒彻骨，怎得梅花扑鼻香。",
		"from": "《上堂开示颂》",
		"by": "黄蘖禅师",
		"time": ["2019.8.20"]
	},
	{
		"text": "君歌杨叛儿，妾劝新丰酒。",
		"from": "《杨叛儿》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "万条千缕绿相迎。舞烟眠雨过清明。",
		"from": "《浣溪沙·二月和风到碧城》",
		"by": "晏几道",
		"time": ["2019.8.20"]
	},
	{
		"text": "是他春带愁来，春归何处。",
		"from": "《祝英台近·晚春》",
		"by": "辛弃疾",
		"time": ["2019.8.20"]
	},
	{
		"text": "儿童漫相忆，行路岂知难。",
		"from": "《倪庄中秋》",
		"by": "元好问",
		"time": ["2019.8.20"]
	},
	{
		"text": "开花必早落，桃李不如松。",
		"from": "《箜篌谣》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "今年元夜时，月与灯依旧。",
		"from": "《生查子·元夕》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "劝君终日酩酊醉，酒不到刘伶坟上土。",
		"from": "《将进酒》",
		"by": "李贺",
		"time": ["2019.8.20"]
	},
	{
		"text": "飞流直下三千尺，疑是银河落九天。",
		"from": "《望庐山瀑布》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "竹外桃花三两枝，春江水暖鸭先知。",
		"from": "《惠崇春江晚景 / 惠崇春江晓景》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "可怜白骨攒孤冢，尽为将军觅战功。",
		"from": "《吊万人冢》",
		"by": "张蠙",
		"time": ["2019.8.20"]
	},
	{
		"text": "对菱花、与说相思，看谁瘦损。",
		"from": "《瑞鹤仙·梅》",
		"by": "陆叡",
		"time": ["2019.8.20"]
	},
	{
		"text": "遮莫圆明似前度，不知谁续广寒游。",
		"from": "《闰中秋玩月》",
		"by": "慧霖",
		"time": ["2019.8.20"]
	},
	{
		"text": "香炉初上日，瀑水喷成虹。",
		"from": "《彭蠡湖中望庐山》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "呵笔难临帖，敲床且煮茶。",
		"from": "《暮雪》",
		"by": "大须",
		"time": ["2019.8.20"]
	},
	{
		"text": "西登香炉峰，南见瀑布水。",
		"from": "《望庐山瀑布水二首》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "鱼泽滩头嘶猎马，省嵬城畔看黄河。",
		"from": "《九月九日登长城关》",
		"by": "石茂华",
		"time": ["2019.8.20"]
	},
	{
		"text": "落红不是无情物，化作春泥更护花。",
		"from": "《己亥杂诗·其五》",
		"by": "龚自珍",
		"time": ["2019.8.20"]
	},
	{
		"text": "一骑红尘妃子笑，无人知是荔枝来。",
		"from": "《过华清宫绝句三首》",
		"by": "杜牧",
		"time": ["2019.8.20"]
	},
	{
		"text": "袨服华妆着处逢，六街灯火闹儿童。",
		"from": "《京都元夕》",
		"by": "元好问",
		"time": ["2019.8.20"]
	},
	{
		"text": "水际轻烟，沙边微雨。荷花芳草垂杨渡。",
		"from": "《踏莎行·闲游》",
		"by": "刘将孙",
		"time": ["2019.8.20"]
	},
	{
		"text": "愿天上人间，占得欢娱，年年今夜。",
		"from": "《二郎神·炎光谢》",
		"by": "柳永",
		"time": ["2019.8.20"]
	},
	{
		"text": "报答春光知有处，应须美酒送生涯。",
		"from": "《江畔独步寻花七绝句》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "关山万里不可越，谁能坐对芳菲月。",
		"from": "《从军行》",
		"by": "卢思道",
		"time": ["2019.8.20"]
	},
	{
		"text": "不识庐山真面目，只缘身在此山中。",
		"from": "《题西林壁》",
		"by": "苏轼",
		"time": ["2019.8.20"]
	},
	{
		"text": "万树寒无色，南枝独有花。",
		"from": "《早梅》",
		"by": "道源",
		"time": ["2019.8.20"]
	},
	{
		"text": "墉集欺猫鼠，林藏逐雀鹯。",
		"from": "《中秋咏怀借杜子美秋日述怀一百韵和寄柳州假鸣桑先生》",
		"by": "徐威",
		"time": ["2019.8.20"]
	},
	{
		"text": "飞来山上千寻塔，闻说鸡鸣见日升。",
		"from": "《登飞来峰》",
		"by": "王安石",
		"time": ["2019.8.20"]
	},
	{
		"text": "旧时王谢、堂前燕子，飞向谁家。",
		"from": "《人月圆·南朝千古伤心事》",
		"by": "吴激",
		"time": ["2019.8.20"]
	},
	{
		"text": "陪都歌舞迎佳节，遥祝延安景物华。",
		"from": "《元旦口占用柳亚子怀人韵》",
		"by": "董必武",
		"time": ["2019.8.20"]
	},
	{
		"text": "剑外忽传收蓟北，初闻涕泪满衣裳。",
		"from": "《闻官军收河南河北》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "红深绿暗径相交，抱暖含芳披紫袍。",
		"from": "《寒食日作》",
		"by": "温庭筠",
		"time": ["2019.8.20"]
	},
	{
		"text": "玉门山嶂几千重，山北山南总是烽。",
		"from": "《从军行七首》",
		"by": "王昌龄",
		"time": ["2019.8.20"]
	},
	{
		"text": "洛阳城里春光好，洛阳才子他乡老。",
		"from": "《菩萨蛮》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "雨后寒轻，风前香软，春在梨花。",
		"from": "《柳梢青·吴中》",
		"by": "仲殊",
		"time": ["2019.8.20"]
	},
	{
		"text": "东边日出西边雨，道是无晴却有晴。",
		"from": "《竹枝词二首·其一》",
		"by": "刘禹锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "重阳独酌杯中酒，抱病起登江上台。",
		"from": "《九日五首·其一》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "竹影和诗瘦，梅花入梦香。",
		"from": "《绝句·竹影和诗瘦》",
		"by": "王庭筠",
		"time": ["2019.8.20"]
	},
	{
		"text": "深居俯夹城，春去夏犹清。",
		"from": "《晚晴》",
		"by": "李商隐",
		"time": ["2019.8.20"]
	},
	{
		"text": "年年端午风兼雨，似为屈原陈昔冤。",
		"from": "《端午三首》",
		"by": "赵蕃",
		"time": ["2019.8.20"]
	},
	{
		"text": "天秋月又满，城阙夜千重。",
		"from": "《客夜与故人偶集 / 江乡故人偶集客舍》",
		"by": "戴叔伦",
		"time": ["2019.8.20"]
	},
	{
		"text": "白马绕旌旗，悲鸣相追攀。",
		"from": "《豫章行》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "墉集欺猫鼠，林藏逐雀鹯。",
		"from": "《中秋咏怀借杜子美秋日述怀一百韵和寄柳州假鸣桑先生》",
		"by": "徐威",
		"time": ["2019.8.20"]
	},
	{
		"text": "是无猫邪，是不会蓄猫也。",
		"from": "《世无良猫》",
		"by": "乐钧",
		"time": ["2019.8.20"]
	},
	{
		"text": "花落家童未扫，莺啼山客犹眠。",
		"from": "《田园乐七首·其六》",
		"by": "王维",
		"time": ["2019.8.20"]
	},
	{
		"text": "乃幽武置大窖中，绝不饮食。天雨雪。",
		"from": "《苏武传(节选)》",
		"by": "班固",
		"time": ["2019.8.20"]
	},
	{
		"text": "一骑红尘妃子笑，无人知是荔枝来。",
		"from": "《过华清宫绝句三首》",
		"by": "杜牧",
		"time": ["2019.8.20"]
	},
	{
		"text": "穿天透地不辞劳，到底方知出处高。",
		"from": "《咏瀑布》",
		"by": "冯云山",
		"time": ["2019.8.20"]
	},
	{
		"text": "何人解赏西湖好，佳景无时。",
		"from": "《采桑子·何人解赏西湖好》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "淮水东边旧时月，夜深还过女墙来。",
		"from": "《金陵五题·石头城》",
		"by": "刘禹锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "泛菊杯深，吹梅角远，同在京城。",
		"from": "《柳梢青·送卢梅坡》",
		"by": "刘过",
		"time": ["2019.8.20"]
	},
	{
		"text": "凭觞静忆去年秋，桐落故溪头。",
		"from": "《诉衷情·凭觞静忆去年秋》",
		"by": "晏几道",
		"time": ["2019.8.20"]
	},
	{
		"text": "七夕年年信不违，银河清浅白云微，蟾光鹊影伯劳飞。",
		"from": "《浣溪沙·七夕年年信不违》",
		"by": "毛文锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "还怕掩、深院梨花，又作故人清泪。",
		"from": "《无闷·催雪》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "扬子江头杨柳春，杨花愁杀渡江人。",
		"from": "《淮上与友人别》",
		"by": "郑谷",
		"time": ["2019.8.20"]
	},
	{
		"text": "春无踪迹谁知。除非问取黄鹂。",
		"from": "《清平乐·春归何处》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "一夜娇啼缘底事，为嫌衣少缕金华。",
		"from": "《与小女》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "碧毯线头抽早稻，青罗裙带展新蒲。",
		"from": "《春题湖上》",
		"by": "白居易",
		"time": ["2019.8.20"]
	},
	{
		"text": "新竹高于旧竹枝，全凭老干为扶持。",
		"from": "《新竹》",
		"by": "郑燮",
		"time": ["2019.8.20"]
	},
	{
		"text": "柳外轻雷池上雨，雨声滴碎荷声。小楼西角断虹明。",
		"from": "《临江仙·柳外轻雷池上雨》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "杨柳青青江水平，闻郎江上踏歌声。",
		"from": "《竹枝词二首·其一》",
		"by": "刘禹锡",
		"time": ["2019.8.20"]
	},
	{
		"text": "听风听雨过清明。愁草瘗花铭。",
		"from": "《风入松·听风听雨过清明》",
		"by": "吴文英",
		"time": ["2019.8.20"]
	},
	{
		"text": "总为浮云能蔽日，长安不见使人愁。",
		"from": "《登金陵凤凰台》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "登鸾车，侍轩辕，遨游青天中，其乐不可言。",
		"from": "《飞龙引二首·其一》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "休说鲈鱼堪脍，尽西风，季鹰归未？",
		"from": "《水龙吟·登建康赏心亭》",
		"by": "辛弃疾",
		"time": ["2019.8.20"]
	},
	{
		"text": "梅子青，梅子黄，菜肥麦熟养蚕忙。",
		"from": "《首夏山中行吟》",
		"by": "祝允明",
		"time": ["2019.8.20"]
	},
	{
		"text": "军歌应唱大刀环，誓灭胡奴出玉关。",
		"from": "《出塞》",
		"by": "徐锡麟",
		"time": ["2019.8.20"]
	},
	{
		"text": "竹竿有甘苦，我爱抱苦节。",
		"from": "《苦寒吟》",
		"by": "孟郊",
		"time": ["2019.8.20"]
	},
	{
		"text": "江涵雁影梅花瘦，四无尘、雪飞云起，夜窗如昼。",
		"from": "《贺新郎·挽住风前柳》",
		"by": "卢祖皋",
		"time": ["2019.8.20"]
	},
	{
		"text": "朝辞白帝彩云间，千里江陵一日还。",
		"from": "《早发白帝城 / 白帝下江陵》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "雪粉华，舞梨花，再不见烟村四五家。",
		"from": "《大德歌·冬景》",
		"by": "关汉卿",
		"time": ["2019.8.20"]
	},
	{
		"text": "想边鸿孤唳，砌蛩私语。",
		"from": "《扫花游·秋声》",
		"by": "王沂孙",
		"time": ["2019.8.20"]
	},
	{
		"text": "秋风万里芙蓉国，暮雨千家薜荔村。",
		"from": "《秋宿湘江遇雨》",
		"by": "谭用之",
		"time": ["2019.8.20"]
	},
	{
		"text": "小桃灼灼柳鬖鬖，春色满江南。",
		"from": "《诉衷情·小桃灼灼柳鬖鬖》",
		"by": "黄庭坚",
		"time": ["2019.8.20"]
	},
	{
		"text": "半盏屠苏犹未举，灯前小草写桃符。",
		"from": "《除夜雪》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "不见襄阳登览，磨灭游人无数，遗恨黯难收。",
		"from": "《水调歌头·多景楼》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "兰帐玉人睡觉，怪春衣、雪沾琼缀。",
		"from": "《水龙吟·燕忙莺懒芳残》",
		"by": "章楶",
		"time": ["2019.8.20"]
	},
	{
		"text": "正是浴兰时节动。菖蒲酒美清尊共。",
		"from": "《渔家傲·五月榴花妖艳烘》",
		"by": "欧阳修",
		"time": ["2019.8.20"]
	},
	{
		"text": "桑野就耕父，荷锄随牧童。",
		"from": "《田家元日》",
		"by": "孟浩然",
		"time": ["2019.8.20"]
	},
	{
		"text": "路漫漫其修远兮，吾将上下而求索。",
		"from": "《离骚》",
		"by": "屈原",
		"time": ["2019.8.20"]
	},
	{
		"text": "莫唱当年长恨歌，人间亦自有银河。",
		"from": "《马嵬》",
		"by": "袁枚",
		"time": ["2019.8.20"]
	},
	{
		"text": "年年越溪女，相忆采芙蓉。",
		"from": "《春宫怨》",
		"by": "杜荀鹤",
		"time": ["2019.8.20"]
	},
	{
		"text": "乡书不可寄，秋雁又南回。",
		"from": "《章台夜思》",
		"by": "韦庄",
		"time": ["2019.8.20"]
	},
	{
		"text": "秋色到空闺，夜扫梧桐叶。",
		"from": "《卜算子·秋色到空闺》",
		"by": "夏完淳",
		"time": ["2019.8.20"]
	},
	{
		"text": "愿得此身长报国，何须生入玉门关。",
		"from": "《塞上曲二首·其二》",
		"by": "戴叔伦",
		"time": ["2019.8.20"]
	},
	{
		"text": "短篱残菊一枝黄。正是乱山深处、过重阳。",
		"from": "《南歌子·驿路侵斜月》",
		"by": "吕本中",
		"time": ["2019.8.20"]
	},
	{
		"text": "云收雨过波添，楼高水冷瓜甜，绿树阴垂画檐。",
		"from": "《天净沙·夏》",
		"by": "白朴",
		"time": ["2019.8.20"]
	},
	{
		"text": "竹马踉蹡冲淖去，纸鸢跋扈挟风鸣。",
		"from": "《观村童戏溪上》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "去年离别雁初归，今夜裁缝萤已飞。",
		"from": "《闺怨 / 怨诗》",
		"by": "张纮",
		"time": ["2019.8.20"]
	},
	{
		"text": "相思相见知何日？此时此夜难为情！",
		"from": "《三五七言 / 秋风词》",
		"by": "李白",
		"time": ["2019.8.20"]
	},
	{
		"text": "岸雨过城头，黄鹂上戍楼。",
		"from": "《武威春暮闻宇文判官西使还已到晋昌》",
		"by": "岑参",
		"time": ["2019.8.20"]
	},
	{
		"text": "碧水丹山映杖藜，夕阳犹在小桥西。",
		"from": "《题画》",
		"by": "沈周",
		"time": ["2019.8.20"]
	},
	{
		"text": "浊酒不销忧国泪，救时应仗出群才。",
		"from": "《黄海舟中日人索句并见日俄战争地图》",
		"by": "秋瑾",
		"time": ["2019.8.20"]
	},
	{
		"text": "别离还有经年客，怅望不如河鼓星。",
		"from": "《七夕》",
		"by": "徐凝",
		"time": ["2019.8.20"]
	},
	{
		"text": "愿君多采撷，此物最相思。",
		"from": "《相思》",
		"by": "王维",
		"time": ["2019.8.20"]
	},
	{
		"text": "会当凌绝顶，一览众山小。",
		"from": "《望岳》",
		"by": "杜甫",
		"time": ["2019.8.20"]
	},
	{
		"text": "半盏屠苏犹未举，灯前小草写桃符。",
		"from": "《除夜雪》",
		"by": "陆游",
		"time": ["2019.8.20"]
	},
	{
		"text": "白下有山皆绕郭，清明无客不思家。",
		"from": "《清明呈馆中诸公》",
		"by": "高启",
		"time": ["2019.8.20"]
	}
